{
  "version": 2,
  "builds": [
    {
      "src": "api/files.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/files",
      "dest": "/api/files.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
