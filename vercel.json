{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/view",
      "dest": "/api/view.js"
    },
    {
      "src": "/admin",
      "dest": "/api/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
